@model ViewContactUploadModel
@{
    ViewData["Title"] = "ViewUploadContact";
}


<div class="content-wrapper">
    <!-- Content area -->
    <div class="content">
        <div class="row">
            <div class="col-12">
                <div class="pb-3">
                    <h4 class="d-inline">UploadContact Details</h4>
                    <a asp-area="Member" asp-controller="ContactUpload" asp-action="Index" class="btn btn-primary float-right"> <i class="icon-list mr-2"></i>Contact Upload List</a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <!-- Basic datatable -->
                <div class="card">
                    <div class="card-body">
                        <partial name="_ActionMessage" model="@Model.Response" />
                    </div>
                    <!-- Page header -->
                    <div class="row">
                        <div class="col-6">
                            <table class="table table-borderless">
                                <tr>
                                    <td class="col-3 float-left"><h7>File Name : </h7></td>
                                    <td class="col-9 float-left"><h7>@Model.FileName</h7></td>
                                </tr>
                                <tr>
                                    <td class="col-3 float-left"><h7>Processing Status:  </h7></td>
                                    <td class="col-9 float-left"><h7>@Model.IsProcessing</h7></td>
                                </tr>
                            </table>
                        </div>
                        <div class="col-6">
                            <table class="table table-borderless">
                                <tr>
                                    <td class="col-3 float-left"><h7>File Url: </h7></td>
                                    <td class="col-9 float-left"><h7>@Model.FileUrl</h7></td>
                                </tr>
                                <tr>
                                    <td class="col-3 float-left"><h7>Succed Entry: </h7></td>
                                    <td class="col-9 float-left"><h7>@Model.SucceedEntryCount</h7></td>
                                </tr>
                            </table>
                        </div>
                    </div>
                    <!-- /page header -->
                    @*<hr />
                    <input type="hidden" id="userId" value="@Model.UserId" />*@
                    @*<table class="table" id="campaign-table">
                        <thead>
                            <tr>
                                <th>Contact Email</th>
                                <th>Delivered</th>
                                <th>Seen</th>
                                <th>Seen Date Time</th>
                                <th>Send Date Time</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>*@
                </div>
                <!-- /basic datatable -->
            </div>
        </div>
    </div>
    <!-- /content area -->
</div>
<!-- /main content -->

@section Scripts {
    <partial name="~/Views/Shared/_ValidationScriptsPartial.cshtml" />
    <partial name="~/Views/Bundle/_DatatableScriptsPartial.cshtml" />
    <script src="~/js/ajax-call.js"></script>
    <script src="~/js/project/campaignreport.js"></script>
    <script>
        $(function () {
            var campaignId = $('#campaignId').val();
            @*var url = '@Url.Action("ViewDeleveryReport", "Campaigns", new { Area = "Member", campaignId = Model.CampaignId })';
            loadDatatable(url);*@
        });
    </script>
}


